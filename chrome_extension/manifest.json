{
    "manifest_version": 2,

    "name": "PandaTwitch",
    "description": "This extension modifys Panda.tv to be more like Twitch.tv",
    "version": "1.0",

    "page_action": {
        "default_icon": "icon.png"
    },
    "permissions": [
        "http://www.panda.tv/*",
        "https://www.panda.tv/*",
        "tabs",
        "storage"
    ],
    "content_scripts": [{
        "matches": ["http://www.panda.tv/*", "https://www.panda.tv/*"],
        "js": ["js/jquery.min.js", "js/waitForKeyElements.js", "known_associations.js", "ptv.js"],
        "run_at": "document_start"
    }]
}
